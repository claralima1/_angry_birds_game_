#=============================
# Unit Width Pixels: 4
# Unit Height Pixels: 4
# Display Width Pixels: 512
# Display Height Pixels: 256
# Base addr: 0x10010000
#==============================
.text
main: lui $8, 0x1001

#========================================================================================================================================================================
#Backgound
#========================================================================================================================================================================
# Azul
#========================================================================================================================================================================
     addi $16, $0, 1536
     addi $10, $0, 0x4B0082
     
azul: beq $16, $17, amarelin
     sw $10, 0($8)
     sw $10, 33000($8)
     addi $8, $8, 4
     addi $17, $17, 1
     j azul
     
#========================================================================================================================================================================
amarelin:
#========================================================================================================================================================================
     addi $16, $0, 1024
     addi $17, $0, 0
     addi $10, $0, 0x4B0082
     
amarelo1: beq $16, $17, amarelo
     sw $10, 0($8)
     sw $10, 33000($8)
     addi $8, $8, 4
     addi $17, $17, 1
     j amarelo1

#========================================================================================================================================================================
amarelo:
#========================================================================================================================================================================
     addi $16, $0, 1024
     addi $17, $0, 0
     addi $10, $0, 0x4B0082
     
amarelo2: beq $16, $17, verde
     sw $10, 0($8)
     sw $10, 33000($8)
