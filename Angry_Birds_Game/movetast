#========================================================================================================================================================================
nuvem:
#========================================================================================================================================================================
cor: lui $12, 0x1001
     addi $16, $0, 4
     addi $17, $0, 0
     addi $10, $0, 0x4B0082
     addi $13, $12, 0
        
intermediario: addi $12, $13, 2620
     addi $16, $0, 4
     addi $17, $0, 0
     addi $10, $0, 0x4B0082
     j n1
         
n1: beq $16, $17, cont1
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j n1

cont1: addi $12, $12, 492
     addi $16, $0, 6
     addi $17, $0, 0

n2: beq $16, $17, cont2
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j n2

cont2: addi $12, $12, 488
     addi $16, $0, 6
     addi $17, $0, 0

n3: beq $16, $17, cont3
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j n3

cont3:addi $12, $12, 492
     addi $16, $0, 4
     addi $17, $0, 0

n4: beq $16, $17, conferir_nuvem
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j n4
     
conferir_nuvem:
    addi $20, $0, 80
    beq $20, $14, mover_nuvem_contrario
    j mover_nuvem
    
mover_nuvem:
    addi $14, $14, 1
    addi $16, $0, 4
    addi $17, $0, 0
    addi $4, $0, 100
    addi $2,$0, 32
    syscall
    jal apagarn
    addi $13, $13, 4
    j verificar_teclado
    
zerar:
    addi $14, $0, 0
    addi $15, $0,0
    j conferir_nuvem
    
mover_nuvem_contrario:
    addi $20, $0, 80
    beq $15, $20, zerar
    addi $15, $15, 1
    addi $16, $0, 4
    addi $17, $0, 0
    addi $4, $0, 100
    addi $2,$0, 32
    syscall
    jal apagarn
    
    addi $13, $13, -4
    j verificar_teclado
    
apagarn: addi $12, $13, 0
     addi $12, $12, 2620
     addi $16, $0, 4
     addi $17, $0, 0
     addi $10, $0, 0x4B0082
        
apagar_n1: beq $16, $17, apagar_cont1
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j apagar_n1
       
apagar_cont1: addi $12, $12, 492
     addi $16, $0, 6
     addi $17, $0, 0
       
apagar_n2: beq $16, $17, apagar_cont2
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j apagar_n2
       
apagar_cont2: addi $12, $12, 488
       addi $16, $0, 6
       addi $17, $0, 0 
       
apagar_n3: beq $16, $17, apagar_cont3
     sw $10, 0($12)
     addi $12, $12, 4
     addi $17, $17, 1
     j apagar_n3

apagar_cont3: addi $12, $12, 492
       addi $16, $0, 4
       addi $17, $0, 0
       
apagar_n4: beq $16, $17, voltar
       sw $10, 0($12)
       addi $12, $12, 4
       addi $17, $17, 1
       j apagar_n4


#========================================================================================================================================================================
verificar_teclado:
#========================================================================================================================================================================
	lui $1, 0xffff
	lw $3, 4($1)
	addi $19, $0, 'd'
	beq $19, $3, frente
	addi $19, $0, 'w'
	beq $19, $3, subir
	addi $19, $0, 's'
	beq $19, $3, cair
	addi $19, $0, 'a'
	beq $19, $3, pular
	j intermediario

