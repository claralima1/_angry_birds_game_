#========================================================================================================================================================================
peteca:
#========================================================================================================================================================================
     lui $8, 0x1001
     addi $16, $0, 7
     addi $17, $0, 0
     addi $10, $0, 0x0078432a
     
colp0: beq $16, $17, peteca0    
       sw $10, 20052 ($8)
       sw $10, 33000($8)
       addi $8, $8, 512
       addi $17, $17, 1
       j colp0

peteca0: addi $16, $0, 8
         addi $17, $0, 0
         lui $8, 0x1001
         
colp1: beq $16, $17, peteca1 
       sw $10, 20056 ($8)
